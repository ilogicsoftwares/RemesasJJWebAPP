        <script src="~/scripts/AngularExtensions/angularServices.js"></script>
        <script src="~/scripts/AngularExtensions/angularDirectives.js"></script>
        <script src="~/scripts/AngularExtensions/angularControllers.js"></script>
        <script src="~/scripts/angular-route.js"></script>
</div>
